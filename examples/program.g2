fn get_true(): string =>
{
    return "TRUE";
}
fn get_false(): string =>
{
    return "FALSE";
}

fn main(): void =>
{
    let count = 0;
    if (count == 1)
    {
        printf("Count == 0: %s\n", get_true());
    }
    else
    {
        printf("Count == 0: %s\n", get_false());
    }
    count = count + 1;
    if (count == 1)
    {
        printf("Count == 1: %s\n", get_true());
    }
    else
    {
        printf("Count == 1: %s\n", get_false());
    }
    return 0;
}
