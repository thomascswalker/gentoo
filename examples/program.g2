fn main(): void =>
{
    let count = 0;
    if (count)
    {
        printf("TRUE: %d\n", count);
    }
    else
    {
        printf("FALSE: %d\n", count);
    }
    count = count + 1;
    if (count)
    {
        printf("TRUE: %d\n", count);
    }
    else
    {
        printf("FALSE: %d\n", count);
    }
    return 0;
}
